    <meta charset="utf-8" />
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <title>MealVite</title>
    @System.Web.Optimization.Styles.Render("~/Content/bootstrap")
    @System.Web.Optimization.Styles.Render("~/Content/fontawesome")
    @System.Web.Optimization.Styles.Render("~/Content/loadingbar")
    @System.Web.Optimization.Styles.Render("~/Content/adminlte")
</head>
<body>
    @*<div ng-include src="'app/components/header/header.html'"></div>*@
